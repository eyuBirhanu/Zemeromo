{
  "expo": {
    "name": "Zemeromo",
    "slug": "zemeromo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/zemeromo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/zemeromo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0E1204"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.eyubirhanu.zemeromo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/zemeromo.png",
        "backgroundColor": "#0E1204"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Zemeromo to access your music.",
          "savePhotosPermission": "Allow Zemeromo to save music.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "9bfecf0a-5527-4406-b2d2-b0d58c0d0b63"
      },
      "router": {}
    },
    "owner": "eyubirhanu"
  }
}
